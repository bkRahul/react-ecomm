{
  "name": "react-ecomm-server",
  "version": "1.0.0",
  "engines": {
    "node": "14.15.3",
    "npm": "6.14.9"
  },
  "description": "server side code for react-ecomm",
  "main": "server.js",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "build": "cd client && yarn build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "stripe": "^8.150.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkRahul/react-ecomm.git"
  },
  "keywords": [
    "react",
    "ecommerce",
    "react-saga",
    "redux",
    "node.js"
  ],
  "author": "rahul bk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bkRahul/react-ecomm/issues"
  },
  "homepage": "https://github.com/bkRahul/react-ecomm#readme"
}
